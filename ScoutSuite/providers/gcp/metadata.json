{
  "security": {
    "iam": {
      "resources": {
        "service_accounts": {
          "cols": 2,
          "path": "services.iam.projects.id.service_accounts"
        }
      }
    },
    "cloudresourcemanager": {
      "resources": {
        "bindings": {
          "cols": 2,
          "path": "services.cloudresourcemanager.projects.id.bindings"
        }
      }
    }
  },
  "compute": {
    "computeengine": {
      "resources": {
        "instances": {
          "cols": 2,
          "path": "services.computeengine.instances"
        },
        "snapshots": {
          "cols": 2,
          "path": "services.computeengine.snapshots"
        },
        "firewalls": {
          "cols": 2,
          "path": "services.computeengine.firewalls"
        },
        "networks": {
          "cols": 2,
          "path": "services.computeengine.networks"
        },
        "subnetworks": {
          "cols": 2,
          "path": "services.computeengine.subnetworks"
        }
      }
